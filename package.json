{
  "name": "eslint-config-armor",
  "version": "4.7.0",
  "description": "Shared config for Armor's eslint style",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:find:current-rules": "eslint-find-rules -c",
    "eslint:find:all-rules": "eslint-find-rules -a",
    "eslint:find:plugin-rules": "eslint-find-rules -p",
    "eslint:find:unused-rules": "eslint-find-rules -u -n"
  },
  "author": "ait-software",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-promise": "^4.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "eslint-find-rules": "^3.6.1"
  }
}
